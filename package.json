    "b-bitcoin-lib": "browserify node_modules/bitcoinjs-lib/src/index.js -s bitcoinjs-lib | uglifyjs -c > ./pub/libs/bitcoinjs-lib.min.js",
    "b-cryptojs": "browserify node_modules/crypto-js/index.js -s crypto-js | uglifyjs -c > ./pub/libs/crypto-js.min.js",
    "b-buffer": "browserify node_modules/buffer/index.js -s buffer | uglifyjs -c > ./pub/libs/buffer.min.js"
