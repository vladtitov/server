    "socket.io-client": "^2.0.3",
    "sqlite": "^2.5.0",
    "tough-cookie-filestore": "0.0.1",
    "ts-node": "^1.6.0",
    "typescript": "~2.3.3",
    "uuid": "^3.0.1",
    "zone.js": "^0.8.4"
